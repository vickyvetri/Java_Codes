package Looping;

class Example2 {
public static void main(String[] args) {
	Example2 obj = new Example2();
	int[] ar = {1,2,3,4,7};
		obj.find_biggest(ar);
}

private void find_biggest(int[] ar) {
	// TODO Auto-generated method stub
	
	int big = 0; 
	for(int i=0; i<ar.length; i++)
	{
	if(ar[i]>big)
		big = ar[i];
	}
	System.out.println("Biggest is " + big);  
	
} 
}