package Patterns;

public class Patterns {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Patterns obj = new Patterns();
		obj.pattern_1();
	}

	private void pattern_1() {
		// TODO Auto-generated method stub
		for(int row=5;row>=1;row--) {
			for(int col=1;col<=row;col++) {
				System.out.print("* ");
			}
			System.out.println();
		}
	}

}
