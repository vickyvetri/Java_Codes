package Looping;

public class Example2 {
public static void main(String[] args) {
	Example2 obj = new Example2();
	obj.Print();
}

private void Print() {
	for(int r=1;r<=7;r++){
		if(r==1 || r==7){
			for(int s=1;s<=7;s++){
				System.out.print(". ");
			}
		}
			else{
				System.out.print("* ");
			}
		}
		}
}
