package Looping;

class Example2 {
public static void main(String[] args) {
	Example2 obj = new Example2();
	for(int i=1;i<=5;i++) {
		obj.Prime_Number(i);
	}
}

private void Prime_Number(int no) {
	int count = 0;
	for(int div=2;div<=no/2;div++) {
		if(no%div==0) {
			count++;
			System.out.println(no+"- Not Prime");
			break;
		}
	}
	if(count==0) {
	System.out.println(no+"- Prime Number");
	}
}
}

















