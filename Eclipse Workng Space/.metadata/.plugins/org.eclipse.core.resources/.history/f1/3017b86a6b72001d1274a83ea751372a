package Looping;

public class Example1 {

public static void main(String[] args) {
	Example1 obj = new Example1();
	obj.print();
}

public void print(){
  for(int row=1;row<=5;row++) {
	  for(int col=1;col<=5;col++) {
		  if(col==1 || col==5){
			  System.out.print(". ");
		  }	  
		  else {
			  System.out.print("* ");
		  }
	  }
	  System.out.println();
 }
}
}

