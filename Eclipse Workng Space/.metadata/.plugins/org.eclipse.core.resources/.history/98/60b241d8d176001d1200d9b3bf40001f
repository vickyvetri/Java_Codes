package Looping;

class Example2 {
public static void main(String[] args) {
	Example2 obj = new Example2();
	char[] ar = {'v','i','y','c','k','y'};
		obj.find_biggest(ar);
}

private void find_biggest(char[] ar) {
	// TODO Auto-generated method stub
			char ch = 'y';
			for(int i=0; i<ar.length;i++) {
				if(ar[i]=='y') {
				System.out.print("Yes Presented At position "+i+'\n');
				break;
				}
			} 
}
}