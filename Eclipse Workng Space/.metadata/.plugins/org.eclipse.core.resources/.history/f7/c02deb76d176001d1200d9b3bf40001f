package Looping;

class Example2 {
public static void main(String[] args) {
	Example2 obj = new Example2();
	char[] ar = {'v','i','c','k','y'};
		obj.find_biggest(ar);
}

private void find_biggest(char[] ar) {
	// TODO Auto-generated method stub
			int len = ar.length; 
			for(int i=len-1; i>=0;i--) {
				System.out.print(ar[i]+" ");

			} 
}
}