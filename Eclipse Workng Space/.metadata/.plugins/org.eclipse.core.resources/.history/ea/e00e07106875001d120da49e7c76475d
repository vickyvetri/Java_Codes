package Git;

public class Square_Root {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Square_Root obj = new Square_Root();
		obj.print(49,2);
	}

	private void print(int m,int n) {
		// TODO Auto-generated method stub
		while(n<=m/2) {
			if(m/n ==n) {
				System.out.println(n);
				break;
			}
			n++;
		}
	}
}