package Looping;

//import java.util.Scanner;

class Example2 {
public static void main(String[] args) {
	Example2 obj = new Example2();
	obj.print();
}

private void print() {
	for(int row=1;row<=7;row++) {
		for(int col=1;col<=7;col++) {
			System.out.print("* ");
		}
		System.out.println();
	}
}
}