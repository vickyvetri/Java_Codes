package Looping;

class Example2 {
public static void main(String[] args) {
	Example2 obj = new Example2();
		obj.Print();
}

private void Print() {
	for(int row=1; row<=5; row++){
		for(int col= 1; col<row; col++)
		{
		System.out.print(col+" ");     
		}
		for(int star=1; star<=6-row; star++)
		{	System.out.print("* "); }
		    System.out.println(); 
		}
}
}